<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Deeplink to flow steps -->
    <string name="deeplink_flow_step_list">https://curlybraces.dev/flowpro/flow/{flowId}/steps?stepType={stepType}</string>
    <string name="deeplink_flow_steps_path_param_flow_id">flowId</string>
    <string name="deeplink_flow_steps_query_param_step_type">stepType</string>
    <string-array name="deeplink_flow_steps_query_param_step_type">
        <item>input</item>
        <item>output</item>
    </string-array>

    <!-- Deeplink to flow step details -->
    <string name="deeplink_flow_step_details">https://curlybraces.dev/flowpro/flow/{flowId}/step/{stepId}</string>
    <string name="deeplink_flow_step_details_path_param_flow_id">flowId</string>
    <string name="deeplink_flow_step_details_path_param_step_id">stepId</string>

    <!-- Deeplink to new flow title -->
    <string name="deeplink_newflow_title">https://curlybraces.dev/flowpro/newflow/title</string>

    <!-- Deeplink to newflow select node -->
    <string name="deeplink_newflow_selectnode">https://curlybraces.dev/flowpro/flow/{flowId}/step/selectnode</string>
    <string name="deeplink_newflow_selectnode_path_param_flow_id">flowId</string>
</resources>